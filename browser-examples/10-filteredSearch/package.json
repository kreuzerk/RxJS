    "url": "https://github.com/DoodleScheduling/FE-kreuzerk/issues"
  },
  "homepage": "https://github.com/DoodleScheduling/FE-kreuzerk#readme",
  "dependencies": {
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.24.1",
    "babelify": "^7.3.0",
    "browserify": "^14.4.0",
    "browserify-css": "^0.10.1",
    "http-server": "^0.10.0",
    "moment": "^2.18.1",
